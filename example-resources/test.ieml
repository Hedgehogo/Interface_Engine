object: = BoxScroll:
	object: = Text:
		text-blocks:
			- = TextBlock:
				text:
					directory: > ../../example-resources/
					language: > ru
					key: > linux-readme
		font: > ../../example-resources/segoeui.ttf
		font-size: 20
		resizer: = TextResizer:
			align: > center
			algorithm: > base
	normal-object-position: [0, 0]
